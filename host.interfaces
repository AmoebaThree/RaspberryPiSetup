auto lo

iface lo inet loopback
iface eth0 inet dhcp

allow-hotplug wlan0
auto wlan0

iface wlan0 inet static
        address 192.168.0.1
        netmask 255.255.255.0
        dns-nameservers 8.8.8.8 8.8.4.4

allow-hotplug wlan1
auto wlan1

iface wlan1 inet dhcp
        wpa-ssid "#"
        wpa-psk "#"
        dns-nameservers 8.8.8.8 8.8.4.4

up iptables-restore < /etc/iptables.ipv4.nat
